# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/claude_web

# JWT Configuration
JWT_SECRET=your-secret-key-here

# Server Configuration
PORT=12021
NODE_ENV=development

# Container Configuration
# Note: CONTAINER_MODE is now managed through dynamic configuration
# Use: npm run config:set container_mode true
CONTAINER_HOST=unix:///var/run/docker.sock
CONTAINER_IMAGE=claude-web-dev:latest

# Session Buffer Configuration
# Note: These values are now managed through dynamic configuration
# Use: npm run config:set max_output_buffer 5000
# MAX_OUTPUT_BUFFER=5000
# MAX_OUTPUT_BUFFER_MB=5
# RECONNECT_HISTORY_SIZE=500

# SSHpiper Configuration (for VS Code Remote-SSH)
SSHPIPER_WORKINGDIR=/var/sshpiper

# GitHub OAuth Configuration
# Note: These values are now managed through dynamic configuration
# Use: npm run config:set github_client_id "your-client-id"
# GITHUB_CLIENT_ID=
# GITHUB_CLIENT_SECRET=
# GITHUB_CALLBACK_URL=http://localhost:12021/api/auth/github/callback

# Invite Code System
# Note: This is now managed through dynamic configuration
# Use: npm run config:set require_invite_code true
# REQUIRE_INVITE_CODE=false