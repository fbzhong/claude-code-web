{
  "name": "claude-web",
  "version": "0.1.0",
  "description": "Web-based remote development environment for Claude Code and VS Code",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm --filter backend dev\" \"pnpm --filter frontend start\"",
    "dev:backend": "pnpm --filter backend dev",
    "dev:frontend": "pnpm --filter frontend start",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:e2e": "pnpm --filter tests test:e2e",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install"
  },
  "keywords": [
    "claude",
    "web-terminal",
    "remote-development",
    "vscode",
    "ttyd"
  ],
  "author": "Robin Zhong",
  "license": "Apache-2.0",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/fbzhong/claude-web.git"
  },
  "bugs": {
    "url": "https://github.com/fbzhong/claude-web/issues"
  },
  "homepage": "https://github.com/fbzhong/claude-web#readme"
}